# Créer .env.example
cat > .env.example << 'EOF'
# Application
APP_NAME=Système de Recouvrement Intelligent
APP_VERSION=1.0.0
DEBUG=True
ENVIRONMENT=development

# Database
DB_USER=recouvrement_user
DB_PASSWORD=change-me
DB_NAME=recouvrement_db
DATABASE_URL=postgresql://recouvrement_user:change-me@postgres:5432/recouvrement_db

# Redis
REDIS_URL=redis://redis:6379/0

# JWT
SECRET_KEY=change-me-super-secret-key
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# CORS
ALLOWED_ORIGINS=http://localhost:3000

# pgAdmin
PGADMIN_EMAIL=admin@example.com
PGADMIN_PASSWORD=admin123
EOF